{
  "type": "module",
  "version": "2.0.1",
  "name": "a-js-tools",
  "private": true,
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "build": "jja rm dist && pnpm run b && pnpm run clean:package",
    "build:local": "jja rm dist && node scripts/build-local.js",
    "clean": "jja tm node_modules package-lock.json pnpm-lock.yaml && pnpm install && pnpm run build",
    "dev": "jja rm .eg && rollup --config rollup.dev.config.js && node .eg/index.mjs",
    "diff": "jja pkg --diff",
    "eslint": "jja cls && eslint src",
    "beautify": "pnpm run prettier",
    "prettier": "jja cls && prettier . --write",
    "push:version": "gvv",
    "push": "gvv",
    "vjj": "vjj -b",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@qqi/check-version": "^1.0.5",
    "@qqi/copy-text": "^1.0.7",
    "@qqi/dev": "^0.1.1",
    "@qqi/rollup-external": "^1.0.11",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/expect": "^24.3.2",
    "@types/node": "^25.0.3",
    "a-node-tools": "^4.2.20",
    "color-pen": "^2.0.13",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.5.0",
    "expect": "^30.2.0",
    "globals": "^16.5.0",
    "gvv": "^0.1.8",
    "husky": "^9.1.7",
    "jja": "^2.3.21",
    "lint-staged": "^16.2.7",
    "pjj": "^1.0.4",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vjj": "^1.0.11"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "a-type-of-js": "^2.0.0"
  }
}
